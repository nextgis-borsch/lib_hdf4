set(@EXPORT_PACKAGE_NAME_UPPER@_VERSION @VERSION@)

file (READ "${CMAKE_CURRENT_LIST_DIR}/@EXPORT_PACKAGE_NAME@-exports.cmake" _file_content)
string (REPLACE "IMPORTED)" "IMPORTED GLOBAL)" _file_content "${_file_content}")
file(WRITE "${CMAKE_CURRENT_LIST_DIR}/@EXPORT_PACKAGE_NAME@-exports.cmake" "${_file_content}")

include("${CMAKE_CURRENT_LIST_DIR}/@EXPORT_PACKAGE_NAME@-exports.cmake")

foreach(EXPORT_PACKAGE_LIB_NAME @EXPORT_PACKAGE_LIB_NAMES@)
    set_target_properties(${EXPORT_PACKAGE_LIB_NAME} PROPERTIES INTERFACE_LINK_LIBRARIES "")
endforeach(EXPORT_PACKAGE_LIB_NAME)

set(@EXPORT_PACKAGE_NAME_UPPER@_INCLUDE_DIR "@EXPORT_PACKAGE_INC_DIRS@")
set(@EXPORT_PACKAGE_NAME_UPPER@_LIBRARY @EXPORT_PACKAGE_LIB_NAMES@)
set(@EXPORT_PACKAGE_NAME_UPPER@_LIBRARIES ${@EXPORT_PACKAGE_NAME_UPPER@_LIBRARY})
set(@EXPORT_PACKAGE_NAME_UPPER@_INCLUDE_DIRS ${@EXPORT_PACKAGE_NAME_UPPER@_INCLUDE_DIR})

set(@EXPORT_PACKAGE_NAME_UPPER@_FOUND TRUE)
