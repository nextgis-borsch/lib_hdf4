# - Config file for the @PACKAGE_UPPER_NAME@ package
# It defines the following variables
#  @PACKAGE_UPPER_NAME@_INCLUDE_DIRS - include directories for @PACKAGE_NAME@
#  @PACKAGE_UPPER_NAME@_LIBRARIES    - libraries to link against
#  @PACKAGE_UPPER_NAME@_VERSION      - @PACKAGE_NAME@ version

# Our library dependencies (contains definitions for IMPORTED targets)
if((NOT TARGET @HDF_TARGET_NAME@ OR NOT TARGET @MFHDF_TARGET_NAME@) AND NOT @PACKAGE_UPPER_NAME@_BINARY_DIR)
  include("${CMAKE_CURRENT_LIST_DIR}/@PACKAGE_UPPER_NAME@Targets.cmake")
endif()

get_property(HDF_INCLUDE_DIRS TARGET @HDF_TARGET_NAME@ PROPERTY INTERFACE_INCLUDE_DIRECTORIES)
get_property(MFHDF_INCLUDE_DIRS TARGET @MFHDF_TARGET_NAME@ PROPERTY INTERFACE_INCLUDE_DIRECTORIES)
set(@PACKAGE_UPPER_NAME@_INCLUDE_DIRS ${MFHDF_INCLUDE_DIRS} ${HDF_INCLUDE_DIRS})

# These are IMPORTED targets created by @PACKAGE_UPPER_NAME@Targets.cmake
set(@PACKAGE_UPPER_NAME@_LIBRARIES @HDF_TARGET_NAME@ @MFHDF_TARGET_NAME@)

if(TARGET @XDR_TARGET_NAME@)
    get_property(XDR_INCLUDE_DIRS TARGET @XDR_TARGET_NAME@ PROPERTY INTERFACE_INCLUDE_DIRECTORIES)
    set(@PACKAGE_UPPER_NAME@_INCLUDE_DIRS ${@PACKAGE_UPPER_NAME@_INCLUDE_DIRS} ${XDR_INCLUDE_DIRS})
    set(@PACKAGE_UPPER_NAME@_LIBRARIES ${@PACKAGE_UPPER_NAME@_LIBRARIES} @XDR_TARGET_NAME@)
endif()

set(@PACKAGE_UPPER_NAME@_VERSION "@VERSION@")
set(@PACKAGE_UPPER_NAME@_FOUND TRUE)
